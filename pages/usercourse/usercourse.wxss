.course{
  width: 100%;
  height: 100px;
  /* border: solid 1px; */
  display: flex;
  align-items: center;
  justify-content: space-between;
}
.part1{
  width: 100%;
  height: 1px;
  background-color: rgb(217, 227, 236);
}
.courseImg{
  width: 70px;
  height: 70px;
  margin-left: 15px;
}
.imgView{
  width: 25%;
  height: 70px;
  /* border: solid 1px; */
}
.textView{
  width: 75%;
  height: 70px;
  display: flex;
  align-items: center;
  /* border: solid 1px; */
}
.part2{
  width: 90%;
  height: 1px;
  background-color: rgb(217, 227, 236);
  margin: 0 auto;
}
.textDesc{
  width: 78%;
  height: 70px;
  display: flex;
  flex-direction: column;
}
.finish{
  width: 22%;
  height: 70px;
  display: flex;
  flex-direction: column;
  /* align-items: center; */
  /* justify-content: center; */
}
#studyLesson{
  color: rgb(110, 130, 145);
  font-size: 14px;
}
.finishShow{
  width: 40px;
  height: 40px;
  border: solid 1px rgb(55, 186, 247);
  border-radius: 50%;
  display: flex;
  align-items: center;
  justify-content: center;
}
#finishShow{
  color: rgb(55, 186, 247);
  font-size: 12px;
}
#text1{
  color: rgb(55, 186, 247);
  font-size: 12px;
  margin-left: 8px;
  margin-top: 5px;
}